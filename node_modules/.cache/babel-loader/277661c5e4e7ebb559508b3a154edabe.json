\n            <input type=\"text\" v-model=\"username\" class=\"input\" v-on:keyup.enter=\"verifyLogin()\">\r\n        </div>\r\n        <div>\r\n            Password:\r\n            <input type=\"password\" v-model=\"password\" class=\"input\" v-on:keyup.enter=\"verifyLogin()\">\r\n        </div>\r\n        <div>\r\n            <button @click=\"verifyLogin()\" class=\"btn btn-outline-dark\">Login</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    name:'login',\r\n    data(){\r\n        return{\r\n            username:\"\",\r\n            password:\"\",\r\n            errore:false,\r\n            user:null\r\n        }\r\n    }, \r\n    beforeCreate(){\r\n        if(this.$session.exists()){this.$router.push(\"/user\")}\r\n    },\r\n    methods:{\r\n        verifyLogin(){\r\n            this.user=this.$store.getters.getUserByLogin(this.username,this.password)\r\n            if(this.user==null){this.errore=true}\r\n            else{\r\n                this.errore=false;\r\n                this.$session.start();\r\n                this.$session.set('user',this.user)\r\n                this.$router.push(\"/user\")\r\n                this.$router.go(\"/user\")\r\n\r\n            }\r\n        },\r\n        islog(){\r\n             return this.$session.exists()\r\n        }\r\n\r\n    },\r\n    watch:{\r\n        username: function() {\r\n            this.errore=false\r\n        },\r\n        password: function(){\r\n            this.errore=false\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.login{\r\n    background-color: rgba(128, 128, 128, 0.89);\r\n    border-radius: 15px;\r\n    padding: 15px;\r\n    display: grid;\r\n    grid-row-gap: 15px;\r\n\r\n    .alert-danger{\r\n        border-radius:10px;\r\n        padding: 10px;\r\n    }\r\n    .input{\r\n        border-radius:10px;\r\n    }\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}